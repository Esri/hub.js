{
  "name": "@esri/hub-content",
  "version": "8.6.2",
  "description": "Module to interact with ArcGIS Hub Content in Node.js and modern browsers.",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/content.umd.js",
  "module": "dist/esm/index.js",
  "es2017": "dist/es2017/index.js",
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "dependencies": {
    "fast-xml-parser": "~3.2.4",
    "tslib": "^1.13.0"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-auth": "^2.13.0 || 3",
    "@esri/arcgis-rest-portal": "^2.18.0 || 3",
    "@esri/arcgis-rest-request": "^2.13.0 || 3",
    "@esri/hub-common": "^8.6.0"
  },
  "devDependencies": {
    "@esri/arcgis-rest-auth": "^3.1.1",
    "@esri/arcgis-rest-portal": "^3.1.2",
    "@esri/arcgis-rest-request": "^3.1.1",
    "@esri/hub-common": "^8.6.2",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "jsonapi-typescript": "^0.1.3",
    "rollup": "^2.26.5",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.8.1"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "prepare-skipped": "npm run build",
    "build": "npm run build:node && npm run build:esm",
    "build:esm": "tsc --outDir ./dist/es2017  --declaration --declarationDir ./dist/types",
    "build:esm:es5": "tsc --module es2015 --target es5 --outDir ./dist/esm",
    "build:node": "tsc --module commonjs --outDir ./dist/node",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "dev:esm": "npm run build:esm -- -w",
    "dev:node": "npm run build:node -- -w",
    "format": "prettier --write ./{src,test}/**/*.ts",
    "format:check": "prettier --check ./{src,test}/**/*.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:Esri/hub.js",
  "contributors": [
    {
      "name": "Dave Bouwman",
      "email": "dbouwman@esri.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/hub.js/issues"
  },
  "homepage": "https://github.com/Esri/hub.js#readme",
  "gitHead": "37f3a4e0e1c26399f6597697601d115d727540d9",
  "volta": {
    "extends": "../../package.json"
  }
}
